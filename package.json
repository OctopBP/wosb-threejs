{
    "name": "wosb",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --open",
        "build": "tsc && vite build",
        "build:analyze": "tsc && vite build --mode analyze",
        "preview": "tsc && vite build && vite preview --host --open",
        "bundle-size": "npx vite-bundle-analyzer dist",
        "lint": "biome lint .",
        "lint:fix": "biome lint --write .",
        "format": "biome format .",
        "format:fix": "biome format --write .",
        "check": "biome check .",
        "check:fix": "biome check --write .",
        "prepare": "husky"
    },
    "typeRoots": [
        "node_modules/@types",
        "src/@types"
    ],
    "devDependencies": {
        "@biomejs/biome": "^2.1.1",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.11",
        "rollup-plugin-visualizer": "^5.14.0",
        "terser": "^5.43.1",
        "typescript": "^5.3.3",
        "vite": "^5.0.11"
    },
    "lint-staged": {
        "*.{ts,js,json}": [
            "biome check --write"
        ]
    },
    "dependencies": {
        "@babylonjs/core": "^8.16.0",
        "@babylonjs/inspector": "^8.16.0",
        "@babylonjs/loaders": "^8.16.0"
    }
}
